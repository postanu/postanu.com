{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": ["esnext", "dom"],
    "types": ["vite/client"]
  },
  "include": [
    "./env.d.ts",
    "./src/**/*.ts",
    "./src/**/*.vue",
    "./vite.config.ts"
  ],
}
